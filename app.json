{
  "expo": {
    "name": "Manga Vault",
    "slug": "manga-vault",
    "scheme": "manga-vault",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo-new.jpg",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundImage": "./assets/images/logo-new.jpg"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": true,
      "package": "com.evrayem100.mangavault"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/logo-new.jpg"
    },
    "plugins": ["expo-asset", "expo-router", "expo-web-browser"],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "ed1b0bb3-127f-46aa-b234-835ddb7d5eb2"
      }
    }
  }
}
