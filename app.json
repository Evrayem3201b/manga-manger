{
  "expo": {
    "name": "Manga Vault",
    "slug": "manga-vault",
    "scheme": "manga-vault",
    "version": "1.6.1",
    "orientation": "portrait",
    "icon": "./assets/images/Logo.jpg",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundImage": "./assets/images/Logo.jpg"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": true,
      "package": "com.evrayem100.mangavault"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/Logo.jpg"
    },
    "plugins": ["expo-asset", "expo-router", "expo-web-browser"],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "1f108233-472b-4dda-895f-32dba93729a1"
      }
    }
  }
}
